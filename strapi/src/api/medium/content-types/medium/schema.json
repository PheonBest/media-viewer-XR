    },
    "info": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::medium-info.medium-info"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "mediums"
    },
    "pages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::page.page",
      "mappedBy": "medium"
    },
    "coverCredit": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
