{
  "kind": "collectionType",
  "collectionName": "readers",
  "info": {
    "singularName": "reader",
    "pluralName": "readers",
    "displayName": "Reader",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "userName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
