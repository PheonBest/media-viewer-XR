{
  "kind": "collectionType",
  "collectionName": "tags",
  "info": {
    "singularName": "tag",
    "pluralName": "tags",
    "displayName": "Tag",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "unique": false,
      "required": true
    },
    "color": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "regex": "^#(?:[0-9a-fA-F]{3}|[0-9a-fA-F]{6})$"
    },
    "mediums": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::medium.medium",
      "mappedBy": "tags"
    }
  }
